{
  "name": "ngx-validator",
  "version": "11.0.0",
  "description": "Angular7+ form validator, make error tips easy and automatic. don't need to manually write error tips templates.",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:docs",
    "start:docs": "docgeni serve --port 4900",
    "build-docs": "docgeni serve --skip-site",
    "build": "ng build core --configuration production",
    "build:docs": "docgeni build --configuration production --base-href=/ngx-validator/",
    "pub-only": "npm publish ./dist/core --access=public",
    "pub": "wpm publish",
    "pull-latest": "git checkout master && git pull origin master",
    "release": "wpm release",
    "test": "ng test core",
    "test:demo": "ng test integration",
    "lint": "ng lint core",
    "lint:demo": "ng lint integration",
    "e2e": "ng e2e",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "validator",
    "angular",
    "form",
    "validation",
    "validate"
  ],
  "private": false,
  "repository": "https://github.com/why520crazy/ngx-validator",
  "author": "why520crazy <mail@why520crazy@163.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "lib": "^3.0.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@docgeni/cli": "^1.1.5",
    "@docgeni/template": "^1.1.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@worktile/pkg-manager": "0.0.1",
    "all-contributors-cli": "^6.6.0",
    "coveralls": "^3.0.9",
    "highlight.js": "^10.4.1",
    "husky": "^2.3.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^12.2.7",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.1",
    "ts-node": "~7.0.0",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
